# Example .env file for local development
# Copy this to `.env` and fill the values (do NOT commit `.env`)

# Postgres
POSTGRES_USER=yellbook
POSTGRES_PASSWORD=yellbook
POSTGRES_DB=yellbook
POSTGRES_PORT=5432

# Redis
# (docker-compose uses the `redis` service by default)
REDIS_HOST=redis
REDIS_PORT=6379

# Backend URLs
NEXT_PUBLIC_BACKEND_URL=http://api:3001
INTERNAL_BACKEND_URL=http://127.0.0.1:3001

# Third-party keys
# AI key (Gemini embeddings)
# Preferred:
GEMINI_API_KEY= AIzaSyAeIualqVpX4hr6CR1tC6yAAoPwLap1Hlk
# Legacy (still supported by the API code):
OPENAI_API_KEY=

# GitHub OAuth (do NOT commit real secrets)
GITHUB_ID=
GITHUB_SECRET=

# NextAuth secret (generate with: openssl rand -base64 32)
NEXTAUTH_SECRET=

# Database URL (optional override; if set, will be used by apps not running in compose)
# DATABASE_URL=postgresql://user:password@localhost:5432/yellbook
