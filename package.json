{
  "name": "@yellbook/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "typecheck": "npm run typecheck -w @yellbook/web",
    "lint": "npm run lint -w @yellbook/web",
    "dev:api": "npm run dev -w @yellbook/api",
    "dev:web": "npm run dev -w @yellbook/web",
    "dev": "npm run dev:api",
    "build:api": "nx build @yellbook/api",
    "build:web": "nx build @yellbook/web",
    "build": "nx run-many --target=build --projects=@yellbook/api,@yellbook/web --parallel",
    "start:api": "npm run start -w @yellbook/api",
    "start:web": "npm run start -w @yellbook/web",
    "clean": "nx reset",
    "clean:deps": "rm -rf node_modules apps/*/node_modules && bun install"
  },
  "private": true,
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@fastify/autoload": "~6.0.3",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/sensible": "~6.0.2",
    "@prisma/client": "^6.18.0",
    "@tanstack/react-query-next-experimental": "^5.90.2",
    "axios": "^1.13.2",
    "fastify": "~5.2.1",
    "fastify-plugin": "~5.0.1",
    "next": "^14.2.33",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^5.10.0",
    "styled-jsx": "^5.1.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "^9.8.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@nx/esbuild": "21.5.3",
    "@nx/eslint": "21.5.3",
    "@nx/eslint-plugin": "^21.5.3",
    "@nx/js": "21.5.3",
    "@nx/next": "^21.5.3",
    "@nx/node": "^21.5.3",
    "@nx/workspace": "21.5.3",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^20.19.33",
    "@types/react": "^19.0.0",
    "@types/react-dom": "19.0.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "^21.6.2",
    "postcss": "^8.5.6",
    "prettier": "^2.8.8",
    "prisma": "^6.18.0",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.3.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "workspaces": [
    "apps/*",
    "libs/contract",
    "libs/config",
